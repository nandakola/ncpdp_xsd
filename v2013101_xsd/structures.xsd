	<!--  RateOfAdministration  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="RateOfAdministration">
		<xsd:sequence>
			<xsd:element name="RateOfAdministration" type="datatypes:an1..11"/>
			<xsd:element name="RateUnitOfMeasure" type="datatypes:SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as seconds, minutes, hours, days.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimePeriodBasis" type="datatypes:SigCode">
				<xsd:annotation>
					<xsd:documentation>Examples such as minute, hour, day. For example, a dosing basis unit of measure of mg/kg to be calculated for a 24 hour time period would have a final representation of mg/kg/day.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
