	<!--  CFMedicationLotNotUsedInCompoundIngredient  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CFMedicationLotNotUsedInCompoundIngredient">
		<xsd:complexContent>
			<xsd:restriction base="structures:SpecializedMedication">
				<xsd:sequence>
					<xsd:element name="DrugDescription" type="datatypes:an1..105">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DrugCoded" type="datatypes:CFDrugCoded"/>
					<xsd:element name="Quantity" type="datatypes:Quantity">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DaysSupply" type="datatypes:n1..3">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="WrittenDate" type="datatypes:DateType">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LastFillDate" type="datatypes:DateType">
						<xsd:annotation>
							<xsd:documentation>Value LD.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ExpirationDate" type="datatypes:DateType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EffectiveDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PeriodEnd" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeliveredOnDate" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DateValidated" type="datatypes:DateType" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
