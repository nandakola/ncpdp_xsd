	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Medication  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Medication">
		<xsd:annotation>
			<xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DrugDescription" type="datatypes:an1..105">
				<xsd:annotation>
					<xsd:documentation>Is the self-contained full drug name, strength, and form. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugCoded" type="datatypes:DrugCoded" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Quantity" type="datatypes:Quantity" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DaysSupply" type="datatypes:n1..3" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WrittenDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value 85.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LastFillDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value LD.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value 36.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EffectiveDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value 07.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PeriodEnd" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value  PE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeliveredOnDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value BE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DateValidated" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value BE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NumberOfRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Diagnosis" type="structures:Diagnosis" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="CouponNumber" type="datatypes:an1..35" minOccurs="0"/>
			<xsd:element name="PriorAuthorization" type="datatypes:an1..35" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Note" type="datatypes:an1..210" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OtherCompoundInformation" type="structures:OtherCompoundInformationType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Sig" type="structures:Sig" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Compound" type="datatypes:CompoundIngredient" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SoldDate" type="datatypes:DateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalRefillsAuthorized" type="datatypes:n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value A. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PharmacyRequestedRefills" type="datatypes:n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DoNotFill" type="ecl:DoNotFillCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeededNoLaterThanDate" type="datatypes:UtcDateType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeZone" type="datatypes:TimeZone" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NeededNoLaterThanReason" type="datatypes:an1..70" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="RefillsRemaining" type="datatypes:n1..2" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HistoryPrescriberOrderNumber" type="datatypes:an1..35" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pharmacy" type="structures:OptionalPharmacy" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Prescriber" type="structures:HistoryPrescriber" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="HistorySource" type="datatypes:HistorySource" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SplitScript" type="ecl:SplitScriptIndicator" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ManufacturerName" type="datatypes:an1..105" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Name of the manufacturer.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LotNumber" type="datatypes:LotNumber" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
