					<xsd:element name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Pharmacy" type="structures:OptionalPharmacy" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="Prescriber" type="structures:HistoryPrescriber" minOccurs="0" maxOccurs="0"/>
					<xsd:element name="HistorySource" type="datatypes:HistorySource" minOccurs="0" maxOccurs="0"/>
