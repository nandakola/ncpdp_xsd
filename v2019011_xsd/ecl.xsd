					<xsd:documentation>The injury is auto related.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="OTHER">
				<xsd:annotation>
					<xsd:documentation>The injury is other property/casualty related.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
