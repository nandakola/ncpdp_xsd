				<xsd:annotation>
					<xsd:documentation>Date of birth of patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType">
				<xsd:annotation>
					<xsd:documentation>Address of the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Communication numbers of the patient.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PatientLocation" type="PatientLocation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Location within a facility where the patient is located.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PatientType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PatientType">
