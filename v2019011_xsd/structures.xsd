							<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Ethnicity" type="CDCCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SubstanceUse" type="SubstanceUseType" minOccurs="0"/>
					<xsd:element name="AlternateContact" type="AlternateContactStructure" minOccurs="0"/>
					<xsd:element name="GestationalAge" type="n1..2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Gestational age at birth (number of weeks)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="HospiceIndicator" type="HospiceIndicator" minOccurs="0">
