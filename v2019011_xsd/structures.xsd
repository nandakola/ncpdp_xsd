					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Name" type="NameType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FormerName" type="NameType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberAgent" type="NameComposite" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberPlaceOfService" type="n2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the place where professional services are rendered resulting in the prescription/order. Required when place of service for the encounter is documented in the patient record.  Otherwise, required when needed to comply with state or federal programs. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PrescriberChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PrescriberChoice">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="Prescriber"/>
			<xsd:element name="Veterinarian" type="VetPrescriberMirrorPrescriber"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PrescriberGeneral  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PrescriberGeneral">
		<xsd:complexContent>
			<xsd:restriction base="HistoryPrescriber">
				<xsd:sequence>
					<xsd:element name="Identification" type="NonVeterinarianID" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Specialty" type="an1..10" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Specialty of prescriber. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Name" type="NameType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="FormerName" type="NameType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Address" type="AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberAgent" type="NameComposite" minOccurs="0"/>
					<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType">
						<xsd:annotation>
							<xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrescriberPlaceOfService" type="n2" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the place where professional services are rendered resulting in the prescription/order. Required when place of service for the encounter is documented in the patient record.  Otherwise, required when needed to comply with state or federal programs. The CMS Place of Service code set as the ECL values https://www.cms.gov/Medicare/Coding/place-of-service-codes/Place_of_Service_Code_Set.html</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PrescriberGeneralChoice  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PrescriberGeneralChoice">
		<xsd:choice>
			<xsd:element name="NonVeterinarian" type="PrescriberGeneral"/>
			<xsd:element name="Veterinarian" type="VetPrescriberMirrorPrescriberGeneral"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PrescriberPANotification  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PrescriberPANotification">
		<xsd:sequence>
			<xsd:element name="Identification" type="NonVeterinarianID">
				<xsd:annotation>
					<xsd:documentation>Identification of the prescriber.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="NameType">
				<xsd:annotation>
					<xsd:documentation>Name of the prescriber.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType">
				<xsd:annotation>
					<xsd:documentation>Address of the prescriber.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Communication numbers of the prescriber.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PriorPeriodCorrectionType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PriorPeriodCorrectionType">
		<xsd:sequence>
			<xsd:element name="CorrectionDate" type="StartEndDateType" minOccurs="0"/>
			<xsd:element name="CorrectionType" type="an" minOccurs="0"/>
			<xsd:element name="BenefitsCoordination" type="BenefitsCoordinationForCensus" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Provider  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="Provider">
		<xsd:annotation>
			<xsd:documentation>Used when medical provider is needed, not the prescriber. Designated Provider - The patient’s provider for which this service applies, if applicable. The designated provider may be the primary care provider, the medical home provider, the facility provider or other provider. This information is provided as reference.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="NonVeterinarianID" minOccurs="0"/>
			<xsd:element name="Specialty" type="an1..10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specialty of provider. Health Care Provider Taxonomy Code http://www.wpc-edi.com/reference/</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PracticeLocation" type="PracticeLocationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" type="NameType"/>
			<xsd:element name="FormerName" type="NameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0"/>
			<xsd:element name="CommunicationNumbers" type="CommunicationNumbersType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailCommonType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailCommonType">
		<xsd:sequence>
			<xsd:element name="QuestionID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the question within the question set. Must be unique per question in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SequenceNumber" type="n1..3">
				<xsd:annotation>
					<xsd:documentation>Sequence number assigned by the payer to indicate the order of the question in relation to other questions in the question set. Must be unique per question in the transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionLevel" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Question level to indicate grouping of questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionText" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Question text to display to the provider.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodedReference" type="CodedReference" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="DefaultNextQuestionID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A QuestionID or value of "END" to identify the next question when it is not dependent on the answer to the question. Required when the QuestionType is date or free text or when the QuestionType is select or numeric and the NextQuestionID for the answer choices or numeric conditions are not provided.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailForRequestType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailForRequestType">
		<xsd:complexContent>
			<xsd:extension base="QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="QuestionTypeForRequest"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionDetailForResponseType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionDetailForResponseType">
		<xsd:complexContent>
			<xsd:extension base="QuestionDetailCommonType">
				<xsd:sequence>
					<xsd:element name="QuestionType" type="QuestionType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroup  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroup">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeader"/>
			<xsd:element name="Question" type="QuestionDetailForResponseType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroupForRequest  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroupForRequest">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeaderWithComment"/>
			<xsd:element name="Question" type="QuestionDetailForRequestType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetGroupForResponse  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetGroupForResponse">
		<xsd:sequence>
			<xsd:element name="Header" type="QuestionSetHeader"/>
			<xsd:element name="Question" type="QuestionDetailForResponseType" maxOccurs="unbounded"/>
			<xsd:element name="DeadlineForReply" type="DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetHeader  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetHeader">
		<xsd:sequence>
			<xsd:element name="QuestionSetID" type="an1..70">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the payer to identify the question set. It should not be assumed this is a unique identifier over time or across payers.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetTitle" type="an1..70">
				<xsd:annotation>
					<xsd:documentation>Title of the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetDescription" type="an1..2000">
				<xsd:annotation>
					<xsd:documentation>Descriptive information about the question set.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSetContactCommunicationNumber" type="CommunicationNumbersTypeRestricted"/>
			<xsd:element name="AttachmentRequired" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>An indicator an attachment is required.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AttachmentNotes" type="an1..70" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Required if AttachmentRequired value is ‘Yes’. Description of the info required.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionSetHeaderWithComment  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionSetHeaderWithComment">
		<xsd:complexContent>
			<xsd:extension base="QuestionSetHeader">
				<xsd:sequence>
					<xsd:element name="QuestionSetComment" type="an1..2000" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The vendor should always allow the provider to send comments in addition to answering the questions.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionType">
		<xsd:choice>
			<xsd:element name="Select" type="PASelectType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="DateTypeFlag">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="NumericTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="FreeTextTypeBasic">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  QuestionTypeForRequest  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="QuestionTypeForRequest">
		<xsd:choice>
			<xsd:element name="Select" type="PASelectTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" type="DateTypeForRequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Numeric" type="NumericChoiceForPARequest">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FreeText" type="AnswerForTextType">
				<xsd:annotation>
					<xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSApprovedDetail  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSApprovedDetail">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSDeniedType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSDeniedType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSModelType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSModelType">
		<xsd:sequence>
			<xsd:element name="SolicitedModel" type="REMSSolicitedType"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSOpenType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSOpenType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DeadlineForReply" type="DateType">
				<xsd:annotation>
					<xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QuestionSet" type="QuestionSetGroup" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSOpenTypeResponse  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSOpenTypeResponse">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MoreInformationRequired" type="QuestionSetGroupForResponse"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSReasonClosedCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSReasonClosedCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ClosedReasonCode" type="ReasonCode20" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Code indicating the reason for the prior authorization closure.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSReasonCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSReasonCodeType">
		<xsd:sequence>
			<xsd:element name="REMSCaseID" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReasonCode" type="ReasonCode20" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>Code used to clarify the content of a message.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="REMSAuthorizationNumber" type="an1..140" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationDetail" type="PAAuthorizationDetails" minOccurs="0" maxOccurs="5"/>
			<xsd:element name="AuthorizationPeriod" type="DateGroupWithEffectiveExpiredDate" minOccurs="0"/>
			<xsd:element name="REMSNote" type="an1..2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseChoiceType  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseChoiceType">
		<xsd:choice>
			<xsd:element name="Approved" type="REMSApprovedDetail">
				<xsd:annotation>
					<xsd:documentation>Either AuthorizationDetail or AuthorizationPeriod must be sent.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Denied" type="REMSDeniedType"/>
			<xsd:element name="Open" type="REMSOpenTypeResponse"/>
			<xsd:element name="Closed" type="REMSReasonClosedCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseChoiceTypeRestricted  <<choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseChoiceTypeRestricted">
		<xsd:choice>
			<xsd:element name="Open" type="REMSOpenType"/>
			<xsd:element name="Closed" type="REMSReasonCodeType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  REMSResponseStructureCommon  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="REMSResponseStructureCommon">
		<xsd:sequence>
			<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Transaction Key. Used in conjunction with same field described above in the RequestReferenceNumber. The Mailbox may return the positional key of the specific piece of mail within their files.</xsd:documentation>
				</xsd:annotation>
