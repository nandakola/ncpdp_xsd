	<xsd:complexType name="MTMServiceResponseDeniedType">
		<xsd:complexContent>
			<xsd:restriction base="ADeniedAbstractType">
				<xsd:sequence>
					<xsd:element name="ReasonCode" type="ReasonCode4" maxOccurs="10">
						<xsd:annotation>
							<xsd:documentation>Code used to clarify the content of a message.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ReferenceNumber" type="an1..35" minOccurs="0">
						<xsd:annotation>
